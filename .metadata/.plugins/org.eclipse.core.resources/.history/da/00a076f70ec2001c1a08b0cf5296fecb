package array;

public class If_Ex1 {
	
	public static void main(String[] args) {
		int score = 88;
		char grade;
	
		if(score > 90) {
			grade = 'A';
			System.out.println("A학점입니다.");
		} else if(score >= 80) {
			System.out.println("B학점입니다.");
		} else if(score >= 70) {
			System.out.println("C학점입니다.");
		} else if(score >= 60) {
			grade = 'D';
		} else {
			grade = 'F';
		}
		
		System.out.println(score + "점 은 " + grade + "학점 입니다.");
	}
}
