package individual_practice;

abstract class SmartPhone {
	String call;
	String sns;
	String search;
	String game;
	String company, name, color, size, weight, price;
	
	// 일반 메서드
	public void purpose() {
		System.out.println("사용목적 : " + call + " / " + sns + " / " + search + " / " + game);
	}
	
	// 추상 메서드(오버라이딩 필수)
	abstract public void spec();	
}
