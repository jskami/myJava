package sort;

public class ShellSort {

	public static void intervalSort(int a[], int begin, int end, int interval) {
		int j;
		for(int i = begin + interval; i <= end; i = interval) {
			int item = a[i];
			for(j = i - interval; j >= begin && item < a[j]; j = j - interval) {
				a[j + interval] = a[j];
			}
			a[j + interval] = item;
		}
	}
	
	
	
}
