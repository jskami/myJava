package sort;

public class SelectSort {
	// 연습
	public int[] select(int n, int[] arr) {
		for (int i = 0; i < n-1; i++) {
			int idx = i;
			
			for (int j = i + 1; j < n; j++) {
				if (arr[idx] > arr[j]) {
					idx = j;
				}
			}
			int tmp = arr[i];
			arr[i] = arr[idx];
			arr[idx] = tmp;
		}
		return arr;
	}
	
	public static void main(String[] args) {
		int[] arr = {3, 1, 5, 10, 12, 9};
		
		for (int i : arr)
		System.out.println(arr);
	}
}
