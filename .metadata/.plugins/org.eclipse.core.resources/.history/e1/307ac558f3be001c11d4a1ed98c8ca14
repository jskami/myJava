package search;

public class LinearSearch {
	static int lSearch(int[] a, int n, int key) {
		int i = 0;
		
		while(true) {
			if(i == n)
				return -1;
			if(a[i] == key)
				return i;
			i++;
		}
		
		// for문 구현
		int result = -1;
		for(int i = 0; i < n; i++) {
			if(a[i] == key) {
				result = i;
			}
		}
		return result;
		
		
		
	}
}