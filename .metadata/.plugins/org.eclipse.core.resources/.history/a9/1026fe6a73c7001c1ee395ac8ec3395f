package individual_practice;

abstract class Galaxy extends SmartPhone {
	public void spec()	{
		company = "삼성";
		name = "갤럭시s20";
		color = "블루";
		size = "13cm";
		weight = "152g";
		price = "98만원";
		System.out.println(company + "/" + name + "/" + color + "/" + size + "/" + weight + "/" + price);
	}
}
