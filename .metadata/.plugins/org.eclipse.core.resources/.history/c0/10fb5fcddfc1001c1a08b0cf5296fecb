package array;

	// 연습하기 : 다형성 
	class Student {
		public void Homework() {
			System.out.println("공부해라");
		}
	}
	
	class kid extends Student {
		public void Homework() {
			System.out.println("아이도 공부해");
		}
	}
	
	class senior extends Student {
		public void Homework() {
			System.out.println("어른도 공부해");
		}
	}

	class alien extends Student {
		public void Homework() {
			System.out.println("외계인도 공부해");
		}
	}
	
	public class StudentTest99 {
		public static void main(String[] args) {
			StudentTest99 st = new StudentTest99();
			st.homework(new kid());
			st.homework(new senior());
			st.homework(new alien());
		}
		
		public void homework(Student student) {
			student.Homework();
		}

	}
	