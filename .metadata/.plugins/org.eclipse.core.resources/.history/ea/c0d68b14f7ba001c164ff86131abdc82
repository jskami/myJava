package data_structure;

public class InsertionSort {
	
	public static void InsertionSort(int[] arr) {
		for (int i = 1; i < arr.length; i++) {
			int key = arr[i];
			int position = i;
			
			while (position > 0 && key < arr[position - 1]) {
				arr[position] = arr[position - 1];
				position--;
			}
		}
	}
	
}
