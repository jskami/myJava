package data_structure;

import java.util.Scanner;

public class Algorithm {
	/* 알고리즘 예제 1 (백준 1000번)
	 * 두 정수 A, B를 입력받은 후, A + B를 출력하는 프로그램 작성*/
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = 1, b = 2;
		a = sc.nextInt();
		b = sc.nextInt();
		System.out.println(a + b);
	}
}
