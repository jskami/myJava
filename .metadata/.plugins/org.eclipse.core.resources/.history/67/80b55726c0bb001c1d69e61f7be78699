package collection.treeset;

public class Tree {
	int count;
	
	public Tree() {
		count = 0;
	}
	
	public class Node {
		Object data;
		Node left;
		Node right;
		
		// 생성시 매개변수를 받아 초기화하는 방법으로만 선언 가능
		public Node(Object data) {
			this.data = data;
			left = null;
			right = null;
		}
		
		public void addLeft(Node node) {
			left = node;
			count++;
		}
		
		public void addRight(Node node) {
			right = node;
			count++;
		}
		
		public void deleteLeft() {
			left = null;
			count--;
		}
		
		public void deleteRight() {
			rigth = null;
			count--;
		}
		
		
	}
	
	
}
